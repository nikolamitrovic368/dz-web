{
  "name": "zwirnerweb",
  "version": "1.0.0",
  "private": true,
  "keywords": [
    "sanity",
    "template",
    "nextjs"
  ],
  "license": "MIT",
  "main": "package.json",
  "scripts": {
    "build": "next build",
    "dev": "next",
    "format": "npx prettier --write . --ignore-path .gitignore",
    "postinstall": "husky install",
    "lint": "next lint -- --ignore-path .gitignore",
    "lint:fix": "npm run lint -- --fix",
    "start": "next start",
    "sync-dev-db": "sanity dataset export production production.tar.gz --overwrite && sanity dataset import production.tar.gz dev --replace",
    "test": "yarn type-check && yarn jest --runInBand",
    "test-all": "yarn type-check && yarn lint:fix && yarn format",
    "type-check": "tsc --noEmit --pretty"
  },
  "prettier": {
    "bracketSpacing": false,
    "printWidth": 100,
    "semi": false,
    "singleQuote": true
  },
  "dependencies": {
    "@next/font": "^13.1.2",
    "@portabletext/react": "^2.0.0",
    "@sanity/demo": "^1.0.1",
    "@sanity/icons": "^2.2.2",
    "@sanity/image-url": "^1.0.2",
    "@sanity/production-preview": "^2.35.3",
    "@sanity/ui": "^1.0.14",
    "@sanity/vision": "^3.2.4",
    "@tailwindcss/typography": "^0.5.9",
    "next": "^13.1.2",
    "next-sanity": "^4.1.0",
    "next-seo": "^5.15.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "sanity": "^3.3.1",
    "sanity-plugin-asset-source-unsplash": "^1.0.6",
    "sanity-plugin-availability": "^1.0.1",
    "sanity-plugin-iframe-pane": "^2.1.1",
    "styled-components": "^5.3.6",
    "suspend-react": "^0.0.9"
  },
  "devDependencies": {
    "@sanity/color": "^2.2.2",
    "@sanity/eslint-config-studio": "^2.0.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@types/styled-components": "^5.1.26",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.32.0",
    "eslint-config-next": "13.1.2",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "husky": "^8.0.0",
    "jest": "^29.4.1",
    "jest-environment-jsdom": "^29.4.1",
    "postcss": "^8.4.21",
    "prettier": "^2.8.3",
    "prettier-plugin-packagejson": "^2.4.0",
    "prettier-plugin-tailwindcss": "^0.2.1",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.4"
  },
  "packageManager": "yarn@3.4.1"
}
